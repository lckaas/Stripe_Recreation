@page "/"

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=chrome">
    <title>Stripe</title>
    <link rel="stylesheet" href="./app.css">
    <link rel="icon" href="./favicon.ico" type="image/x-icon">
</head>
<body>

    <header class="hero">
        <div class="container">
            <div class="nav-wrapper">
                <a href="/"><img src="/img/stripe-logo.png" width="200" height="55" alt="CTI - Clinical Trials & Consulting Services"></a>
                <button class="mobile-nav-toggle" aria-controls="primary-navigation" aria-expanded="false">
                    <span class="visually-hidden">Menu</span>
                </button>
                <nav class="primary-navigation" id="primary-navigation">
                    <ul aria-label="Primary" role="list" class="nav-list">
                        <li><a href="./wizard">Products</a></li>
                        <li><a href="./tm-wizard">Solutions</a></li>
                        <li><a href="./rave-form-status-wizard">Developers</a></li>
                        <li><a href="./se-settings">Resources</a></li>
                        <li><a href="./se-settings">Pricing</a></li>
                    </ul>
                </nav>
                <button class="button | display-sm-none display-md-inline-flex">Contact Sales ➜</button>
            </div>
            <br />
            <br />
            <div class="row_2-layout">
                <div class="column_2-layout">
                    <div class="hero-title-container">
                        <h1 class="hero-title">Payments infrastructure for the internet</h1>
                        <p class="hero-title" data-overlay aria-hidden="true"></p>
                    </div>
                    <div class="hero-subtitle-container">
                        <p class="hero-subtext">Join the millions of companies of all sizes that use Stripe to accept payments online and in person, embed financial services, power custom revenue models, and build a more profitable business.</p>
                    </div>
                    <input class="hero-email__input" type="text" placeholder="Email address" /><button class="button__input | display-sm-none display-md-inline-flex">Start now ➜</button>
                </div>
                <div class="column_2-layout">
                    <div class="column_2-layout-shift">
                        <img src="/img/stripe-visualization.png" alt="" />
                    </div>
                </div>
            </div>
            <div class="hero-sponsor-container">
                <div class="sponsor__row">
                    <div class="sponsor__column">
                        <div class="sponsor__card">
                            <img src="/img/sponsors/airbnb-2-logo-svgrepo-com.svg" alt="Air bnb" height="80" width="80" />
                        </div>
                    </div>
                    <div class="sponsor__column">
                        <div class="sponsor__card">
                            <img src="/img/sponsors/apple-black-logo-svgrepo-com.svg" alt="Apple" height="80" width="80" />
                        </div>
                    </div>
                    <div class="sponsor__column">
                        <div class="sponsor__card">
                            <img src="/img/sponsors/continental-54-logo-svgrepo-com.svg" alt="Continental" height="120" width="120" />
                        </div>
                    </div>
                    <div class="sponsor__column">
                        <div class="sponsor__card">
                            <img src="/img/sponsors/facebook-messenger-3-logo-svgrepo-com.svg" alt="Facebook" height="80" width="80" />
                        </div>
                    </div>
                </div>
                <br />
                <div class="sponsor__row">
                    <div class="sponsor__column">
                        <div class="sponsor__card">
                            <img src="/img/sponsors/gmail-icon-logo-svgrepo-com.svg" alt="Gmail" height="80" width="80" />
                        </div>
                    </div>
                    <div class="sponsor__column">
                        <div class="sponsor__card">
                            <img src="/img/sponsors/google-icon-logo-svgrepo-com.svg" alt="Google" height="80" width="80" />
                        </div>
                    </div>
                    <div class="sponsor__column">
                        <div class="sponsor__card">
                            <img src="/img/sponsors/sony-2-logo-svgrepo-com.svg" alt="Sony" height="120" width="120" />
                        </div>
                    </div>
                    <div class="sponsor__column">
                        <div class="sponsor__card">
                            <img src="/img/sponsors/united-states-postal-service-logo-svgrepo-com.svg" alt="United States Postal Service" height="120" width="120" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="canvas-container">
            <canvas id="canv" width="32" height="32"></canvas>
        </div>
    </header>

    @* <section>
        <div class="container">
            <h1 class="h1-nc">Title Here</h1>
        </div>
    </section> *@

    <script type="module" src="/js/main.js"></script>
</body>
</html>
